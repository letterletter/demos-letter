    "release": "tsx scripts/release",
    "stylelint": "stylelint '**/*.{css,less}'",
    "test": "npm run test:once -- --watch",
    "test:once": "jest --runInBand --colors --forceExit"
  },
  "lint-staged": {
    "packages/**/*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "packages/**/*.{css,less}": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
